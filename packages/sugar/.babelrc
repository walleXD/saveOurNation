{
  "presets": [
    "es2017",
    "stage-0",
    "react-native"
  ],
  "plugins": [
    "transform-decorators-legacy",
    [
      "module-resolver",
      {
        "alias": {
          "react-native": "react-native-web",
          "react/lib/ReactNativePropRegistry": "react-native-web/dist/modules/ReactNativePropRegistry"
        },
        "extensions": [
          ".web.js",
          ".js"
        ]
      }
    ]
  ]
}
